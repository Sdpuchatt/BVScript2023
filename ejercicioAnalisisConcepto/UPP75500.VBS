NROLEG = LEGAJO.NROLEG
PERIODO = LEGAJO.PERACT
ANIO = LEGAJO.CONCEPTO("VLIQ_ANOLIQ")
TIPLIQ = LEGAJO.CONCEPTO("VLIQ_TIPLIQ")
CLAEMP = LEGAJO.CONCEPTO("VLIQ_CLAEMP")

PER_PAGO = CSTR(LEGAJO.CONCEPTO("CPT_00001")) 'Periodo de Pago (Se paga el periodo siguiente al liquidado)
MES_PAGO = RIGHT(PER_PAGO,2) 'Mes de Pago
ANIO_PAGO = LEFT(PER_PAGO,4) 'Año de pago

'Defino variables
IMPONIBLE_LIQ = LEGAJO.ACUM("GAIMPM")
IMPONIBLE_ANT = LEGAJO.ACUM("GAIMPA")
PRORRATEO_SAC = LEGAJO.CONCEPTO("CPT_75410")
DEDUCCIONES = LEGAJO.ACUM("GADESF")+LEGAJO.ACUM("GADETA")

IMPONIBLE_ANT_46 = LEGAJO.CONCEPTO("CPT_75095")
IMPONIBLE_LIQ_46 = LEGAJO.CONCEPTO("CPT_75145")

'Según Tk. 10703, 11490 si calculan los siguientes conceptos, se aplica exención del 46%
IF (IMPONIBLE_ANT_46 <> 0) THEN
	IMPONIBLE_ANT = IMPONIBLE_ANT_46
END IF

IF (IMPONIBLE_LIQ_46 <> 0) THEN
	IMPONIBLE_LIQ = IMPONIBLE_LIQ_46
END IF

'Cálculo final
IF ((IMPONIBLE_LIQ + IMPONIBLE_ANT + DEDUCCIONES + PRORRATEO_SAC) > 0) THEN
    RESULT = IMPONIBLE_LIQ + IMPONIBLE_ANT + DEDUCCIONES + PRORRATEO_SAC
ELSE
    RESULT = 0
END IF